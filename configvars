CROSSPREFIX=m68k-atari-mint-

CC=$(CROSSPREFIX)gcc
AS=$(CC)
WARN=-Wall -W -Wstrict-prototypes -Wmissing-prototypes -Werror
CFLAGS= -O2 -fomit-frame-pointer $(WARN)
LDFLAGS=-s
LIBS=

HCP = hcp
MKDIR = mkdir -p
PERL = perl

.s.o:
	$(AS) $(CFLAGS) -c -o $@ $<

all::


clean distclean::
	$(RM) *.o *.prg *.pdb *.app *.tos *.ttp

release::
